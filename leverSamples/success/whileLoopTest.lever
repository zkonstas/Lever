/*
	Test for:
	WHILE LOOPS
	Expected output: "YES\nYES\nYES\nYES\n"
*/

program {
	var counter = 1;
	while(counter <= 3) {
		output "YES";
		counter++;
	}

	while(counter <=6)
		counter++;


	if(counter == 7) {
		output "YES";
	}
	else {
		output "NO: braceless while didn't advance counter integer variable to 7\n";
	}
}